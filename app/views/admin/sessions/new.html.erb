<div class="container">
  <div class= "row">

      <h4>管理者ログイン</h4>


         <div class="mx-auto col-11 col-sm-10 col-lg-8 col-xl-6">
          <%= form_for(resource, as: resource_name, url: admin_session_path) do |f| %>

      <table>
      <tr>
            <div class="field">
              <td><%= f.label :email, "メールアドレス" %><br /></td>
              <td><%= f.email_field :email, autofocus: true, autocomplete: "email" %></td>
        </div>
      </tr>

        <tr>
        <div class="field">
          <td><%= f.label :password, "パスワード"%><br /></td>
          <td><%= f.password_field :password, autocomplete: "current-password" %></td>
        </div>
        </tr>
      </table>
        <div class="actions">
          <%= f.submit "ログイン", class: 'btn btn-primary'%>
        </div>
      <% end %>

  </div>
 </div>
</div>

<%#= render "admins/shared/links" %>
