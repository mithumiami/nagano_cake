<h2>商品名編集画面</h2>
        <%= form_with model: @product, url: admin_product_path(@product.id),local: true do |f| %>

           <h4>商品名</h4>
            <%= f.text_field :title %>
          <h4>商品紹介文</h4>
            <%= f.text_area :body %>
          <h4>税抜価格</h4>
            <%= f.text_field :price %>
          <h4>商品画像</h4>


            <%= f.submit '更新' %>
        <% end %>
